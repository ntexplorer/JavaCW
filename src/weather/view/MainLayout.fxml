                                                <VBox layoutX="166.0" layoutY="102.0" prefHeight="419.0"
                                                      prefWidth="627.0" AnchorPane.bottomAnchor="0.0"
                                                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                      AnchorPane.topAnchor="0.0">
